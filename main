    // Update is called once per frame
    void Update() {
        if (Input.GetMouseButtonDown(1)) {
            s10.SetActive(false);
            s100_01.SetActive(false);
            c1.SetActive(false);
            c2.SetActive(false);
            p.SetActive(false);
            StartCoroutine(exemple());
        };
    }   
    

